<!DOCTYPE html>
<html>
    <head>

    </head>
    <body>
        <h1>{{ title }}</h1>
        <p>{{ description }}</p>
        <code>drush {{ title }}</code>
        <h2>Aliases:</h2>
            <ul>
            {% for alias in aliases %}
                <li>{{ alias }}</li>
            {% endfor %}
            </ul>
        <h2>Arguments:</h2>
        {% for argument in arguments %}
            <code>{{ argument['argument'] }} : {{ argument['argumenttext'] }}</code>
        {% endfor %}
        <h2>Options:</h2>
        {% for option in options %}
            <code>--{{ option['option'] }} : {{ option['optiontext'] }}</code>
        {% endfor %}
        <h2>Examples:</h2>
        {% for example in examples %}
            <p>{{ example['exampletext'] }}</p>
            <code>{{ example['example'] }}</code>
        {% endfor %}
    </body>
</html>
